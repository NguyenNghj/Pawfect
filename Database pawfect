DROP TABLE OrderItems;
DROP TABLE Feedbacks;
DROP TABLE DiscountOrders;
DROP TABLE Voucher ;
DROP TABLE PetHotelBookings;
DROP TABLE PetHotel;
DROP TABLE CartItems;
DROP TABLE Products;
DROP TABLE Orders;
DROP TABLE PaymentMethods;
DROP TABLE ShippingMethods;
DROP TABLE Category;
DROP TABLE Pets;
DROP TABLE Customers;
DROP TABLE Staffs;


CREATE TABLE Customers (
    customer_id INT PRIMARY KEY IDENTITY(1,1),
    password NVARCHAR(255) NOT NULL,
    full_name NVARCHAR(100),
    email NVARCHAR(100) UNIQUE NOT NULL,
    phone VARCHAR(15),
    address NVARCHAR(255),
    gender NVARCHAR(10),
    birth_date DATE,
    is_active BIT DEFAULT 1
);

CREATE TABLE Staffs (
    staff_id INT PRIMARY KEY IDENTITY(1,1),
    role_name NVARCHAR(30) NOT NULL,
    password NVARCHAR(255) NOT NULL,
    full_name NVARCHAR(100),
    email NVARCHAR(100) UNIQUE NOT NULL,
    phone VARCHAR(15),
    address NVARCHAR(255),
    gender NVARCHAR(10),
    birth_date DATE,
    image NVARCHAR(MAX),
    is_active BIT DEFAULT 1
);


CREATE TABLE Pets (
    pet_id INT IDENTITY(1,1) PRIMARY KEY,
    customer_id INT NOT NULL FOREIGN KEY REFERENCES Customers(customer_id),
    pet_name NVARCHAR(50) NOT NULL,
    pet_type NVARCHAR(20) NOT NULL,
    pet_breed NVARCHAR(50) NOT NULL,
    pet_sex NVARCHAR(10) NOT NULL,
    pet_weigth DECIMAL(5,2) NOT NULL,
    pet_dob DATE NOT NULL,
    pet_status NVARCHAR(50),
    pet_image NVARCHAR(MAX),
    is_active BIT DEFAULT 1
);


CREATE TABLE Category (
    category_id INT PRIMARY KEY IDENTITY(1,1),
    category_name NVARCHAR(50) NOT NULL,
    is_active BIT DEFAULT 1
);

CREATE TABLE Products (
    product_id INT PRIMARY KEY IDENTITY(1,1),
    category_id INT FOREIGN KEY REFERENCES Category(category_id),
    product_name NVARCHAR(255) NOT NULL,
    product_petType NVARCHAR(30) NOT NULL,
    product_price DECIMAL(12, 2) NOT NULL,
    product_image NVARCHAR(MAX),
    stock INT NOT NULL,
    description NVARCHAR(MAX),
    is_active BIT DEFAULT 1
);



CREATE TABLE PaymentMethods (
    paymentMethod_id INT PRIMARY KEY IDENTITY(1,1),
    name NVARCHAR(50) NOT NULL
);


CREATE TABLE ShippingMethods (
    shippingMethod_id INT PRIMARY KEY IDENTITY(1,1),
    name NVARCHAR(50) NOT NULL,
    shipping_fee DECIMAL(10, 2) NOT NULL
);

CREATE TABLE Orders (
    order_id INT PRIMARY KEY IDENTITY(1,1),
    customer_id INT NOT NULL FOREIGN KEY REFERENCES Customers(customer_id),
    staff_id INT FOREIGN KEY REFERENCES Staffs(staff_id),
    paymentMethod_id INT NOT NULL FOREIGN KEY REFERENCES  PaymentMethods(paymentMethod_id),
    shippingMethod_id INT NOT NULL FOREIGN KEY REFERENCES ShippingMethods(shippingMethod_id),
    recipient_name NVARCHAR(100) NOT NULL,
    recipient_phone NVARCHAR(15) NOT NULL,
    shipping_address NVARCHAR(255) NOT NULL,
    delivery_notes NVARCHAR(MAX),
    total_amount DECIMAL(12, 2) NOT NULL,
	 reason_cancel NVARCHAR(255),
	 request_cancel BIT DEFAULT 0,
    status NVARCHAR(30),
    order_date DATETIME DEFAULT GETDATE(),
    finished_date DATETIME
);


CREATE TABLE OrderItems (
    orderItem_id INT PRIMARY KEY IDENTITY(1,1),
    order_id INT NOT NULL FOREIGN KEY REFERENCES Orders(order_id),
    product_id INT NOT NULL FOREIGN KEY REFERENCES Products(product_id),
    quantity INT NOT NULL
);

CREATE TABLE CartItems (
    cartItem_id INT PRIMARY KEY IDENTITY(1,1),
    customer_id INT NOT NULL FOREIGN KEY REFERENCES Customers(customer_id),
    product_id INT NOT NULL FOREIGN KEY REFERENCES Products(product_id),
    quantity INT NOT NULL
);

CREATE TABLE PetHotel (
    room_id INT PRIMARY KEY IDENTITY(1,1),
    room_name NVARCHAR(100) NOT NULL,
    room_image NVARCHAR(MAX),
    room_type NVARCHAR(20) NOT NULL,
    min_weight DECIMAL(5, 0) NOT NULL,
	 max_weight DECIMAL(5, 0) NOT NULL,
    quantity INT NOT NULL,
 available_quantity INT NOT NULL DEFAULT 0,
    price_per_night DECIMAL(12, 0) NOT NULL,
    description NVARCHAR(MAX) NOT NULL,
    status NVARCHAR(30),
    is_active BIT DEFAULT 1
);

CREATE TABLE PetHotelBookings (
    booking_id INT PRIMARY KEY IDENTITY(1,1),
    room_id INT FOREIGN KEY REFERENCES PetHotel(room_id),
    customer_id INT FOREIGN KEY REFERENCES Customers(customer_id),
    staff_id INT NULL FOREIGN KEY REFERENCES Staffs(staff_id),
    pet_id INT FOREIGN KEY REFERENCES Pets(pet_id),
    check_in DATETIME NOT NULL,
    check_out DATETIME NOT NULL,
    total_price DECIMAL(12, 2) NOT NULL,
	  note NVARCHAR(MAX),
    status NVARCHAR(30),
	  booking_date DATETIME DEFAULT GETDATE(),
    is_active BIT DEFAULT 1	
);

CREATE TABLE Feedbacks (
    feedback_id INT PRIMARY KEY IDENTITY(1,1),
    customer_id INT NOT NULL FOREIGN KEY REFERENCES Customers(customer_id),
    staff_id INT FOREIGN KEY REFERENCES Staffs(staff_id),
    product_id INT NOT NULL FOREIGN KEY REFERENCES Products(product_id),
	 order_id INT NOT NULL FOREIGN KEY REFERENCES Orders(order_id),
    rating INT NOT NULL,
    comment NVARCHAR(MAX),
    reply NVARCHAR(MAX),
    image_path NVARCHAR(MAX),
    feedback_date DATETIME DEFAULT GETDATE(),
    is_visible BIT DEFAULT 1
);

CREATE TABLE Voucher (
    voucher_id INT PRIMARY KEY IDENTITY(1,1),
    code NVARCHAR(50) UNIQUE NOT NULL,
    description NVARCHAR(MAX),
    discount_percentage INT,
    discount_amount DECIMAL(12,2),
    min_order_value DECIMAL(12,2),
    max_discount DECIMAL(12,2),
    start_date DATETIME NOT NULL,
    end_date DATETIME NOT NULL,
    is_active BIT DEFAULT 1
);

CREATE TABLE DiscountOrders (
    discountOrder_id INT PRIMARY KEY IDENTITY(1,1),
    order_id INT NOT NULL FOREIGN KEY REFERENCES Orders(order_id),
    voucher_id INT NOT NULL FOREIGN KEY REFERENCES Voucher(voucher_id),
    discount_amount DECIMAL(12,2) NOT NULL
);




